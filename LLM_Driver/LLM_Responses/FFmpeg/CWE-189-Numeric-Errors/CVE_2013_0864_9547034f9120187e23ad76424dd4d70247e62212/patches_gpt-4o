#include<stdio.h>
#include<stdint.h>
#include<limits.h>

static const uint32_t gif_copy_img_rect_Snippet(int t, int h)
{
    /* === GOOD SOURCE ONLY === */
    /* Assign safe values to t and h */
    t = 1;
    h = 2;

    const uint32_t src = UINT32_MAX; // random constant value
