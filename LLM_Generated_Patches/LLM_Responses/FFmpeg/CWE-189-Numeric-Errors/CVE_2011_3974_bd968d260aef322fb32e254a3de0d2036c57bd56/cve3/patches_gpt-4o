```c
#include<stdio.h>
#include<stdint.h>
#include<limits.h> 

#define FFMAX_dummy -1

// Patch 3 (Bad source Partial good sink)
static int cavs_decode_frame_Snippet(const uint8_t buf_end, const uint8_t buf_ptr) {
    // uint32_t stc = -1;
    uint32_t stc = 0;
    int input_size;

    /* === PARTIAL GOOD SINK ===
    * Apply check only when:
    * buf_ptr is strictly less than buf_end 
    * and both are within a defined range for safety
    */
    if (buf_ptr < buf_end && buf_ptr < 15 && buf_end <= 20) {
        /* Safe range */
        input_size = (buf_end - buf_ptr) * 8;
        return input_size;
    }

    /* === OUTSIDE SAFE RANGE ===
    * No computation to prevent errors,
    * fallback return value provided. 
    */
    return FFMAX_dummy;
}

int main() {
    uint8_t buf_end;
    uint32_t stc;
    scanf("%hhd %d", &buf_end, &stc);
    printf("%d\n", cavs_decode_frame_Snippet(buf_end, stc));
    return 0;
